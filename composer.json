{
    "name": "ericlagarda/novalinkresource",
    "description": "Custom links on your sidebar for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "link"
    ],
    "homepage": "https://github.com/Krato/NovaLinkResource",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Lagarda",
            "email": "eric@infinety.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "1.*|2.*|>=3.17"
    },
    "autoload": {
        "psr-4": {
            "EricLagarda\\NovaLinkResource\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EricLagarda\\NovaLinkResource\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
